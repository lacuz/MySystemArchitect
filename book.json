{
    "title" : "系统架构设计师",
    "author" : "Lacuz",
    "description" : "系统架构设计师考试",
    "language" : "zh-hans",
    "plugins": [
        "splitter",
		"code",
		"hide-element",
		"pageview-count",
		"popup",
		"tbfed-pagefooter",
		"-sharing", "sharing-plus"

    ],
	"pluginsConfig":{
		"hide-element":{
			"elements":[".gitbook-link"]
		},
		"tbfed-pagefooter": {
            "copyright": "Copyright &copy Lacuz 2020",
            "modify_label": "该文章修订时间：",
            "modify_format": "YYYY-MM-DD"
        },
        "sharing": {
            "qq": true,
            "qzone": true,
            "weibo": true,
            "all": [
                "weibo",
                "qq",
                "qzone"
            ]
        }


	}	

	
}